<ul class="text-sm text-gray-500 space-y-4">
<% items.each do |user| %>
  <li class="flex items-center space-x-4">
    <%= link_to user_path(user) do %>
      <%= image_with_fallback(user, :profile_picture, Settings.default.avatar, "w-10 h-10 rounded-full object-cover") %>
    <% end %>
    <%= link_to user.username, user_path(user), class: "text-blue-500 hover:underline" %>
  </li>
<% end %>
</ul>
